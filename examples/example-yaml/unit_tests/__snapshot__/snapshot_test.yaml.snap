Manifest snapshot:
  1: |
    apiVersion: v1
    data:
      game.properties: |-
        enemy:
          types:
          - alien
          - monsters
        player:
          maximumLives:5
    kind: ConfigMap
    metadata:
      name: multiline-yaml-1
  2: |
    apiVersion: v1
    data:
      game.properties: |
        enemy:
          types:
          - imps
          - warriors
        player:
          maximumLives:10
    kind: ConfigMap
    metadata:
      name: multiline-yaml-2
  3: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      name: nested
    spec:
      minReadySeconds: 3
      progressDeadlineSeconds: 60
      revisionHistoryLimit: 5
      selector:
        matchLabels:
          app: nested
      strategy:
        rollingUpdate:
          maxUnavailable: 0
        type: RollingUpdate
      template:
        metadata:
          annotations:
            prometheus.io/port: "9797"
            prometheus.io/scrape: "true"
          labels:
            app: nested
        spec:
          containers:
          - command:
            - ./podinfo
            - --port=9898
            - --port-metrics=9797
            - --grpc-port=9999
            - --grpc-service-name=backend
            - --cache-server=cache:6379
            - --level=info
            env:
            - name: PODINFO_UI_COLOR
              value: '#34577c'
            image: ghcr.io/stefanprodan/podinfo:6.1.1
            imagePullPolicy: IfNotPresent
            livenessProbe:
              exec:
                command:
                - podcli
                - check
                - http
                - localhost:9898/healthz
              initialDelaySeconds: 5
              timeoutSeconds: 5
            name: nested
            ports:
            - containerPort: 1000
              name: grpc
              protocol: TCP
            - containerPort: 9898
              name: http
              protocol: TCP
            - containerPort: 9797
              name: http-metrics
              protocol: TCP
            - containerPort: 10000
              name: extra_port
              protocol: TCP
            readinessProbe:
              exec:
                command:
                - podcli
                - check
                - http
                - localhost:9898/readyz
              initialDelaySeconds: 5
              timeoutSeconds: 5
            resources:
              limits:
                cpu: 2000m
                memory: 512Mi
              requests:
                cpu: 100m
                memory: 32Mi
  4: |
    apiVersion: v1
    data:
      entrypoint.sh: |-
        #!/bin/bash
        echo "Do this"

        echo "Do that"
      game.properties: |-
        ---
        enemy:
          types:
          - alien
          - monsters
        player:
          maximumLives:5
    kind: ConfigMap
    metadata:
      name: my-configmap
  5: |
    apiVersion: v1
    kind: Pod
    metadata:
      name: my-pod
    spec:
      containers:
      - command:
        - sh
        - -c
        - |
          /bin/bash <<'EOF'

          # Do MultiLine YAML Stuff here

          EOF
        image: alpine
        name: my-container
        volumeMounts:
        - mountPath: /bin/entrypoint.sh
          name: configmap-volume
          readOnly: true
          subPath: entrypoint.sh
      volumes:
      - configMap:
          defaultMode: 448
          name: my-configmap
        name: configmap-volume
  6: |
    apiVersion: source.toolkit.fluxcd.io/v1beta1
    kind: HelmRepository
